<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MetroAutomation.FrontPanel"
                    xmlns:controls="clr-namespace:MetroAutomation.Controls">
    <DataTemplate x:Key="SetACI">
        <StackPanel DataContext="{Binding ViewModel, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=local:BaseFrontPanel}}">
            <controls:StateSwitcher IsOn="{Binding LComp}" Content="LCOMP" Command="{Binding ToggleLCompCommand}" />
        </StackPanel>
    </DataTemplate>
    <local:FunctionTemplateSelector x:Key="Fluke5520AExtensionSelector"
                                    SetACI="{StaticResource SetACI}"
                                    />
</ResourceDictionary>